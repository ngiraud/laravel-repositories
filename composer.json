{
    "name": "ngiraud/repositories",
    "description": "Laravel abstract repositories and more",
	"require": {
		"php": ">=5.4.0",
		"illuminate/support": "5.*",
		"illuminate/database": "5.*"
	},
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Giraud",
            "email": "contact@ngiraud.me"
        }
    ],
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"NGiraud\\Repositories\\": "packages/ngiraud/repositories/src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"NGiraud\\Tests\\Repositories\\": "packages/ngiraud/repositories/tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.x-dev"
		}
	},
    "minimum-stability": "dev",
	"prefer-stable": true
}
